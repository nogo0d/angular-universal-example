{
    "name": "was-vanilla",
    "version": "0.0.1",
    "contributors": [
        "Jos√© Monteiro <jose.monteiro@aptoide.com>"
    ],
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "lint": "ng lint --fix",
        
        "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
                
        "clean-dist": "rm -rf dist",
        "build:client": "ng build --env=local --prod",
        "build:server": "ng build --env=local --prod --app 1 --output-hashing=false",
        "build:ssr-server": "npm run webpack:server",
        "build:run": "node dist/server",
        "build": "npm run clean-dist && npm run build:client && npm run build:server && npm run build:ssr-server && npm run build:run"
    },
    "pre-commit": [
        "lint"
    ],
    "private": true,
    "dependencies": {
        "@angular/animations": "^5.0.0",
        "@angular/common": "^5.0.0",
        "@angular/compiler": "^5.0.0",
        "@angular/core": "^5.0.0",
        "@angular/forms": "^5.0.0",
        "@angular/http": "^5.0.0",
        "@angular/platform-browser": "^5.0.0",
        "@angular/platform-browser-dynamic": "^5.0.0",
        "@angular/platform-server": "^5.2.5",
        "@angular/router": "^5.0.0",
        "@nguniversal/common": "^5.0.0-beta.5",
        "@nguniversal/express-engine": "^5.0.0-beta.5",
        "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
        "@ngx-translate/core": "^9.1.1",
        "@ngx-translate/http-loader": "^2.0.1",
        "core-js": "^2.4.1",
        "express-beautify": "^0.1.3",
        "gulp": "^3.9.1",
        "gulp-copy": "^1.1.0",
        "rxjs": "^5.5.2",
        "zone.js": "^0.8.14"
    },
    "devDependencies": {
        "@angular/cli": "^1.5.0",
        "@angular/compiler-cli": "^5.0.0",
        "@angular/language-service": "^5.0.0",
        "@types/node": "^8.0.30",
        "codelyzer": "^4.0.2",
        "cpy-cli": "^1.0.1",
        "express": "^4.15.2",
        "http-server": "^0.10.0",
        "pre-commit": "^1.2.2",
        "reflect-metadata": "^0.1.10",
        "ts-loader": "^3.5.0",
        "tslint": "^5.7.0",
        "typescript": "~2.4.2"
    }
}
